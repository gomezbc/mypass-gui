---
import CredentialEntry from "@/components/CredentialEntry.svelte";

type Credential = {
  domain: string;
  email: string;
  username: string;
  password: string;
};
let credentials: Credential[] = [
  {
    domain: "example.com",
    email: "example@gmail.com",
    username: "johndoe",
    password: "password123",
  },
  {
    domain: "google.com",
    email: "google@gmail.com",
    username: "goglebg",
    password: "googlepass",
  }
];
---

<ul>
  {credentials.map((credential) => <CredentialEntry client:visible {...credential} />)}
</ul>
