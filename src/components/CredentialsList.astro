---
import LoginEntry from "@/components/LoginEntry.svelte";

type Login = {
  domain: string;
  credentials: Credential[];
};
type Credential = {
  email: string;
  username: string;
  password: string;
};
let logins: Login[] = [
  {
    domain: "example.com",
    credentials: [
      {
        email: "example@gmail",
        username: "johndoe",
        password: "password123",
      },
      {
        email: "example2@gmail.com",
        username: "janedoe",
        password: "password123",
      },
    ],
  },
  {
    domain: "google.com",
    credentials: [
      {
        email: "google@gmail.com",
        username: "goglebg",
        password: "googlepass",
      },
    ],
  },
];
---

<ul>
  {logins.map((login) => <LoginEntry client:visible {...login} />)}
</ul>
